local SimpleUi = loadstring(game:HttpGet("https://raw.githubusercontent.com/laagginq/ui-libraries/main/simple-hub/src.lua"))()
local Main = SimpleUi:create('Simply Ass')
local Menu = Main:Menu('ass')
local Page = Menu:Page()
Page:Label('Ass Hub')
Page:Button('Button by ass',false,function()
    print("CenterNo1ee")
end)
Page:Toggle('Toggled by Ass',false,false,false,function(t) -- arg 1 Text , arg 2 ไม่รู้ , arg 3 เปิด - ปิด , arg 4 ไม่รู้ , arg 5 callback
    print(t)
end)
Page:Button('Button by ass',false,function()
    print("CenterNo1")
end)
Page:Slider('ass Slider',1,10,5,function(x)
    print(x)
end)
